    "version": "0.2.0",
    "configurations": [
        {
            "name": "shadower",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/shadower/shadower",
            "args": [
                "configs/config-srsran-n78-20MHz.conf"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "build_all"
        },
        {
            "name": "Tests",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/shadower/test/${fileBasenameNoExtension}",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "build_active"
        },
        {
            "name": "recorder",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/shadower/tools/recorder",
            "args": [
                "-f",
                "3427.5",
                "-n",
                "200000",
                "-s",
                "23.04",
                "-S",
                "23.04",
                "-o",
                "output",
                "-c",
                "2",
                "-t",
                "uhd",
                "-d",
                "type=b200,master_clock_rate=23.04e6"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
        },
        {
            "name": "cutter",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/shadower/tools/cutter",
            "args": [
                "-i",
                "/root/records/output.fc32",
                "-o",
                "/root/records/test.fc32",
                "-s",
                "23.04",
                "-k",
                "133",
                "-n",
                "5"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
        },
        {
            "name": "syncer_test_file",
